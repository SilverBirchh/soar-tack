<div class="jumbotron board">
  <div class="btn-group pull-right" role="group">
    <button type="button" class="btn btn-primary reset" {{action "reset"}}>Reset</button>
  </div>

  <div class="row-container">
    {{ tack-square value=squares.[0] number=0 select="select" }}
    {{ tack-square value=squares.[1] number=1 select="select" }}
    {{ tack-square value=squares.[2] number=2 select="select" }}
  </div>
  <div class="row-container">
    {{ tack-square value=squares.[3] number=3 select="select" }}
    {{ tack-square value=squares.[4] number=4 select="select" }}
    {{ tack-square value=squares.[5] number=5 select="select" }}
  </div>
  <div class="row-container">
    {{ tack-square value=squares.[6] number=6 select="select" }}
    {{ tack-square value=squares.[7] number=7 select="select" }}
    {{ tack-square value=squares.[8] number=8 select="select" }}
  </div>

</div>
